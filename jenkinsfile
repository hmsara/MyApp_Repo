pipeline{
agent any
environment {
        DOCKER_TLS_VERIFY = '0'
    }
stages{
    stage('checkout scm'){
        steps{
            script{
                checkout scmGit(branches: [[name: '*/main']], extensions: [], userRemoteConfigs: [[url: 'https://github.com/hmsara/MyApp_Repo']])}}}
    stage('Build Frontend'){
        steps{
            script{
                
                sh 'docker build -t hmsara/front_image .'
               
            }
        }

    }
     

}
}
